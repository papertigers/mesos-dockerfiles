{
    "id": "/api",
    "cmd": null,
    "cpus": 0.0625,
    "mem": 128,
    "disk": 64,
    "container": {
        "type": "DOCKER",
        "volumes": [],
        "docker": {
            "image": "papertigers/apidemo",
            "network": "BRIDGE",
			"portMappings": [],
